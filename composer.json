{
  "name": "phpffcms/ffcms",
  "version" : "3.0.0",
  "description": "FFCMS 3 version based on MVC core ffcms-core.",
  "keywords": [
    "php",
    "cms",
    "mvc",
    "framework",
    "php5",
    "pdo",
    "ffcms"
  ],
  "type": "project",
  "homepage": "https://ffcms.org",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4",
    "phpffcms/ffcms-core": "dev-master",
    "phpffcms/ffcms-console": "dev-master",
    "phpffcms/ffcms-ckeditor": "dev-master",
    "illuminate/database": "5.*",
    "symfony/http-foundation": "2.6.6",
    "maximebf/debugbar": "1.*",
    "ezyang/htmlpurifier": "4.6.*",
    "gregwar/image": "2.*",
    "dflydev/dot-access-data": "1.0.*",
    "phpffcms/ffcms-elfinder": "*",
    "swiftmailer/swiftmailer": "5.4.*",
    "gregwar/captcha": "1.*",
    "bower-asset/jquery": "2.1.*@stable | 1.11.*@stable",
    "bower-asset/jquery-validation": "1.*@stable",
    "bower-asset/bootstrap": "3.3.* | 3.2.* | 3.1.*",
    "bower-asset/jquery-ui": "1.11.*@stable",
    "bower-asset/components-font-awesome": "4.*@stable"
  },
  "authors": [
    {
      "name": "Mihail Pyatinskyi",
      "email": "zenn1989@gmail.com",
      "homepage": "http://ffcms.ru",
      "role": "Developer"
    }
